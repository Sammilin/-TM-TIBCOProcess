<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.trendmicro.com/ESB/MDM/schemas/SMF/SearchCustomer_Input"
	 xmlns:ns0="http://www.trendmicro.com/ESB/schemas/Common"
	 targetNamespace="http://www.trendmicro.com/ESB/MDM/schemas/SMF/SearchCustomer_Input"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:import namespace="http://www.trendmicro.com/ESB/schemas/Common" schemaLocation="../../../XSD/Common/common.xsd"/>
	<xs:element name="SMF">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SMFHeader" type="ns0:TrendESBCommonHeader"/>
				<xs:element name="SMFBody" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="name" type="xs:string" minOccurs="0"/>
							<xs:element name="Address" type="xs:string" minOccurs="0"/>
							<xs:element name="ExpectedScore" type="xs:int" minOccurs="0"/>
							<xs:element name="BatchId" type="xs:string" minOccurs="0"/>
							<xs:element name="CountryCode" type="xs:string" minOccurs="0"/>
							<xs:element name="FuzzyWeighting" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>