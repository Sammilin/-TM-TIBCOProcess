<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://TrendMicro.BW.Schemas.BW_Properties" xmlns:TM="java://com.trendmicro.TMBWUtilities" xmlns:ns5="http://www.tibco.com/schemas/OrderCreation/XSD/Common/MailInfoSchema.xsd" xmlns:pfx="http://www.trendmicro.com/ESB/schemas/WebServiceList" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns4="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx5="http://www.tibco.com/schemas/PLSService/XSD/ArrowSphere/ErrorProperties.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://www.trendmicro.com/ESB/schemas/ResendProperties.xsd" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.trendmicro.com/ESB/schemas/MailProperties.xsd" xmlns:pfx2="http://www.trendmicro.com/ESB/FRS/schemas/MailProperties.xsd" xmlns:pfx7="http://www.tibco.com/ns/no_namespace_schema_location/XSD/Common/OAuthKeyProperties.xsd" xmlns:pfx6="http://www.tibco.com/schemas/PLSService/XSD/Common/Schema.xsd2">
    <xsd:import namespace="http://TrendMicro.BW.Schemas.BW_Properties" schemaLocation="/resources/Schema/BW_Properties.xsd"/>
    <xsd:import namespace="http://www.trendmicro.com/ESB/schemas/WebServiceList" schemaLocation="/XSD/WebServiceList.xsd"/>
    <xsd:import namespace="http://www.trendmicro.com/ESB/schemas/MailProperties.xsd" schemaLocation="/XSD/Common/MailProperties.xsd"/>
    <xsd:import namespace="http://www.trendmicro.com/ESB/schemas/ResendProperties.xsd" schemaLocation="/XSD/Common/ResendProperties.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/PLSService/XSD/ArrowSphere/ErrorProperties.xsd" schemaLocation="/XSD/ArrowSphere/ErrorProperties.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/PLSService/XSD/Common/Schema.xsd2" schemaLocation="/XSD/Common/ErrorHandling.xsd"/>
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/XSD/Common/OAuthKeyProperties.xsd" schemaLocation="/XSD/Common/OAuthKeyProperties.xsd"/>
    <pd:name>Utilities/OAuth/V1.0/UpdateOAuthProperties.process</pd:name>
    <pd:startName>File Poller</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="File Poller">
        <pd:type>com.tibco.plugin.file.FileEventSource</pd:type>
        <pd:resourceType>ae.activities.FileEventSourceResource</pd:resourceType>
        <pd:x>51</pd:x>
        <pd:y>107</pd:y>
        <config>
            <pollInterval>5</pollInterval>
            <createEvent>false</createEvent>
            <modifyEvent>true</modifyEvent>
            <deleteEvent>false</deleteEvent>
            <mode>files-and-directories</mode>
            <encoding>text</encoding>
            <sortby>File Name</sortby>
            <sortorder>descending</sortorder>
            <encodingUsed>UTF8</encodingUsed>
            <fileName>%%Utilities/OAuth/PropertiesConfigFile%%</fileName>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>494</pd:endX>
    <pd:endY>109</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1240973009344</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>117</pd:x>
        <pd:y>20</pd:y>
        <pd:width>318</pd:width>
        <pd:height>158</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>simpleGroup</pd:groupType>
            <pd:serializable>false</pd:serializable>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Parse Property XML File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>SetVariable</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Parse Property XML File</pd:from>
            <pd:to>SetVariable</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="SetVariable">
            <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
            <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
            <pd:x>339</pd:x>
            <pd:y>106</pd:y>
            <config>
                <variableConfig>/Utilities/OAuth/V1.0/ShareVariable/Shared Variable.sharedvariable</variableConfig>
            </config>
            <pd:inputBindings>
                <xsl:copy-of select="$Parse-Property-XML-File/OAuthKeyProperties"/>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Parse Property XML File">
            <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
            <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
            <pd:x>211</pd:x>
            <pd:y>114</pd:y>
            <config>
                <inputStyle>text</inputStyle>
                <xsdVersion>default</xsdVersion>
                <validateOutput>true</validateOutput>
                <term ref="pfx7:OAuthKeyProperties"/>
            </config>
            <pd:inputBindings>
                <xmlString>
                    <xsl:value-of select="$File-Poller/ns1:EventSourceOuputTextClass/fileContent/textContent"/>
                </xmlString>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Call Send Mail Process">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>486</pd:x>
        <pd:y>220</pd:y>
        <config>
            <processName>/Utilities/MailNotify/Subprocesses/SendMailQueue.process</processName>
            <processNameXPath/>
        </config>
        <pd:inputBindings>
            <mail>
                <to>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Utilities/MailNotify/to"/>
                </to>
                <cc>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Utilities/MailNotify/cc"/>
                </cc>
                <subject>
                    <xsl:value-of select="concat(&quot;[&quot;,$_globalVariables/ns3:GlobalVariables/ESBBW/Environment,&quot;-&quot;,$_globalVariables/ns3:GlobalVariables/Domain,&quot;]&quot;,&#xA;&quot;Reading ErrorMapping Properties Error&quot;)"/>
                </subject>
                <MailBody>
                    <xsl:value-of select="concat(&quot;&lt;BR>&quot;, &#xA;&quot;System raises an exception when processing message. The detailed information as below:&quot;,  &quot;&lt;BR>&quot;,&#xA;&quot;&lt;pre>&quot;,TM:stringReplace(TM:stringReplace(tib:render-xml($_error/ns4:ErrorReport,false(),true()),'&lt;',concat(&quot;&amp;&quot;,&quot;lt;&quot;)),'>',concat(&quot;&amp;&quot;,&quot;gt;&quot;)),&quot;&lt;/pre>&quot;)"/>
                </MailBody>
                <xsl:if test="exists($File-Poller/ns1:EventSourceOuputTextClass/fileContent/textContent)">
                    <AttachFile>
                        <AttachContent>
                            <content-disposition>
                                <xsl:value-of select="concat(&#xA;&quot;attachment; filename=&quot;, $File-Poller/ns1:EventSourceOuputTextClass/fileInfo/fileName)"/>
                            </content-disposition>
                            <content-type>
                                <xsl:value-of select="'text/xml; charset=utf-8'"/>
                            </content-type>
                            <content-transfer-encoding>
                                <xsl:value-of select="'base64'"/>
                            </content-transfer-encoding>
                            <xsl:if test="$File-Poller/ns1:EventSourceOuputTextClass/fileContent/textContent">
                                <textContent>
                                    <xsl:value-of select="$File-Poller/ns1:EventSourceOuputTextClass/fileContent/textContent"/>
                                </textContent>
                            </xsl:if>
                        </AttachContent>
                    </AttachFile>
                </xsl:if>
            </mail>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>54</pd:x>
        <pd:y>216</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Call Send Mail Process</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Call Send Mail Process</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>File Poller</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>File Poller</pd:from>
        <pd:to>Null</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Call Send Mail Process</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>