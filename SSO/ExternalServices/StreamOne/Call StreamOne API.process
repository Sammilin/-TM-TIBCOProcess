<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns2="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns1="http://www.trendmicro.com/ESB/schemas/DBLogger" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:TM="java://com.trendmicro.TMBWUtilities" xmlns:pfx2="http://www.trendmicro.com/ESB/schemas/error" xmlns:pfx="http://www.tibco.com/pe/DeployedVarsType">
    <xsd:import namespace="http://www.trendmicro.com/ESB/schemas/error" schemaLocation="/XSD/Common/error.xsd"/>
    <pd:name>ExternalServices/StreamOne/Call StreamOne API.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="jsonStr" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>136</pd:startX>
    <pd:startY>229</pd:startY>
    <pd:returnBindings>
        <RequestActivityOutput>
            <statusCode>
                <xsl:value-of select="$Non-SSL/RequestActivityOutput/statusLine/statusCode"/>
            </statusCode>
            <xsl:if test="$Non-SSL/RequestActivityOutput/binaryContent">
                <binaryContent>
                    <xsl:value-of select="$Non-SSL/RequestActivityOutput/binaryContent"/>
                </binaryContent>
            </xsl:if>
            <xsl:if test="$Non-SSL/RequestActivityOutput/asciiContent">
                <asciiContent>
                    <xsl:value-of select="$Non-SSL/RequestActivityOutput/asciiContent"/>
                </asciiContent>
            </xsl:if>
        </RequestActivityOutput>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="RequestActivityOutput">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="statusCode" type="xsd:string"/>
                    <xsd:element name="binaryContent" type="xsd:base64Binary" minOccurs="0"/>
                    <xsd:element name="asciiContent" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>708</pd:endX>
    <pd:endY>228</pd:endY>
    <pd:errorSchemas>
        <Error ref="pfx2:Error"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1349954417806</pd:targetNamespace>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>389</pd:x>
        <pd:y>446</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Generate Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>552</pd:x>
        <pd:y>444</pd:y>
        <config>
            <faultName>Error</faultName>
        </config>
        <pd:coercions>
            <pd:coercion xpath="$_error/ns:ErrorReport/Data/*" element="pfx2:Error"/>
        </pd:coercions>
        <pd:inputBindings>
            <ns2:ActivityInput>
                <data>
                    <xsl:choose>
                        <xsl:when test="exists($_error_Generate-Error-1/pfx2:Error)">
                            <xsl:copy-of select="$_error/ns:ErrorReport/Data/pfx2:Error"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <pfx2:Error>
                                <pfx2:src>
                                    <xsl:value-of select="$_error/ns:ErrorReport/ProcessStack"/>
                                </pfx2:src>
                                <pfx2:error_code>
                                    <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                                </pfx2:error_code>
                                <pfx2:error_message>
                                    <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                                </pfx2:error_message>
                                <pfx2:stack_trace>
                                    <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                                </pfx2:stack_trace>
                            </pfx2:Error>
                        </xsl:otherwise>
                    </xsl:choose>
                </data>
            </ns2:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>239</pd:x>
        <pd:y>58</pd:y>
        <pd:width>377</pd:width>
        <pd:height>317</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$Non-SSL/RequestActivityOutput/statusLine/statusCode=200 or $i &gt;10</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Non-SSL</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Non-SSL</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Non-SSL</pd:from>
            <pd:to>Null</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-6750208</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:xpathDescription>Retry</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$i &lt; 10</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>Generate-Error-1</pd:to>
            <pd:xpathDescription>Error</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-6750208</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:activity name="Non-SSL">
            <pd:type>com.tibco.plugin.http.client.HttpRequestActivity</pd:type>
            <pd:resourceType>httppalette.httpRequest</pd:resourceType>
            <pd:x>367</pd:x>
            <pd:y>121</pd:y>
            <config>
                <inputOutputVersion>5.3.0</inputOutputVersion>
                <newMimeSupport>true</newMimeSupport>
                <serverhost>%%ExternalServices/StreamOne/API_Host%%</serverhost>
                <useSsl>false</useSsl>
                <serverport/>
                <ns0:ssl xmlns:ns0="http://www.tibco.com/xmlns/aemeta/services/2002">
                    <ns0:strongCipherSuitesOnly>true</ns0:strongCipherSuitesOnly>
                    <ns0:cert isRef="true">/Certificates/.folder</ns0:cert>
                </ns0:ssl>
                <proxyConnection>/ExternalServices/StreamOne/Proxy Configuration.httpProxy</proxyConnection>
                <InputHeaders>
                    <xsd:element name="root">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="Accept" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Accept-Charset" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Accept-Encoding" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Cookie" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </InputHeaders>
                <OutputHeaders>
                    <xsd:element name="root">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="Allow" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Content-Length" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Content-Encoding" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Date" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                                <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </OutputHeaders>
            </config>
            <pd:inputBindings>
                <RequestActivityInput>
                    <Host>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ExternalServices/StreamOne/API_Host"/>
                    </Host>
                    <Method>
                        <xsl:value-of select="'POST'"/>
                    </Method>
                    <RequestURI>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ExternalServices/StreamOne/API_URI_Billing"/>
                    </RequestURI>
                    <PostData>
                        <xsl:value-of select="concat('data=',TM:URLEncode($Start/root/jsonStr))"/>
                    </PostData>
                    <Headers>
                        <Accept-Encoding>
                            <xsl:value-of select="'indentity'"/>
                        </Accept-Encoding>
                        <Content-Type>
                            <xsl:value-of select="'application/x-www-form-urlencoded'"/>
                        </Content-Type>
                    </Headers>
                </RequestActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>505</pd:x>
            <pd:y>234</pd:y>
            <config/>
            <pd:inputBindings>
                <ns0:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$_globalVariables/pfx:GlobalVariables/ExternalServices/StreamOne/SleepInterval"/>
                    </IntervalInMillisec>
                </ns0:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Null">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>370</pd:x>
            <pd:y>237</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Generate-Error-1">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>459</pd:x>
            <pd:y>317</pd:y>
            <config>
                <faultName>Error</faultName>
            </config>
            <pd:inputBindings>
                <ns2:ActivityInput>
                    <data>
                        <pfx2:Error>
                            <pfx2:src>
                                <xsl:value-of select="$_error/ns:ErrorReport/ProcessStack"/>
                            </pfx2:src>
                            <pfx2:error_code>
                                <xsl:value-of select="$_error/ns:ErrorReport/MsgCode"/>
                            </pfx2:error_code>
                            <pfx2:error_message>
                                <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                            </pfx2:error_message>
                            <pfx2:stack_trace>
                                <xsl:value-of select="$_error/ns:ErrorReport/StackTrace"/>
                            </pfx2:stack_trace>
                        </pfx2:Error>
                    </data>
                </ns2:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>Generate Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>