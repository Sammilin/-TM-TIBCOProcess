		<xs:complexType>
			<xs:all>
				<xs:element name="serviceId" type="xs:string"/>
				<xs:element name="SRNumber" type="xs:string"/>
				<xs:element name="submissionInput" type="xs:string" minOccurs="0"/>
				<xs:element name="isSendMail" type="xs:boolean" minOccurs="0"/>
				<xs:element name="EscDescription" type="xs:string" minOccurs="0"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
</xs:schema>